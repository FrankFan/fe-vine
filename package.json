{
  "name": "fe-vine",
  "version": "1.0.11",
  "description": "oriente hybrid uploader",
  "main": "dist/bundle.js",
  "bin": "bin/cli",
  "scripts": {
    "start": "babel libs --out-dir dist --source-maps cheap-module-eval-source-map --watch",
    "compile": "babel libs --out-dir dist --source-maps cheap-module-eval-source-map",
    "dev": "node bin/cli",
    "prepublish": "rm -rf dist && babel libs --out-dir dist",
    "postinstall": "cp fe-vine.config.json ~/fe-vine.config.json && mv ~/fe-vine.config.json ~/.fe-vine.config.json"
  },
  "author": "oriente.com",
  "license": "ISC",
  "dependencies": {
    "commander": "^2.16.0",
    "exeq": "^3.0.0",
    "chalk": "^2.4.1"
  },
  "keywords": [
    "hybrid",
    "upload",
    "zip"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
}
